---
---
{
  "gitches": [{% for member in site.data.members %}
    {
      "name": "{{ member.name }}",
      "twitter": "{{ member.twitter }}",
      "url": "{{ member.url }}",
      "github": "{{ member.github }}",
      "codepen": "{{ member.codepen }}",
      "email": "{{ member.email }}",
      "gravatar": "{{ member.gravatar }}",
      "title": "{{ member.title }}"{% if member.latlon %},
      "latlon": [{{ member.latlon[0] }}, {{ member.latlon[1] }}]{% endif %}
    }{% if forloop.last %}{% else %},{% endif %}
  {% endfor %}]
}
